            <string>current_level = 0;
//open and read txt
file = file_text_open_read("lvl.txt");
var numlines = 0;
while(!file_text_eof(file))
{
    str[numlines] = file_text_read_string(file);
    file_text_readln(file);
    numlines++;
}
file_text_close(file);
//make levels based on info
l = 0;
for(var t = 0; t &lt; numlines;t++) level[t] = noone;

for(var i = 0; i &lt; numlines;i++)
{
    if(str[i] == "{")
    {
        i++;
        var tempstr = str[i];
        level[l] = instance_create(0,0,struct_level);
        var k = 0;
        while(tempstr != "}")
        {
            level[l].data[k] = tempstr;
            if(string_length(tempstr) &gt; level[l].width)level[l].width = string_length(tempstr);
            i++;
            tempstr = str[i];
            k++
        }
        level[l].height = k;
        l++;
    }
}//for i

//play first level
log("Number of levels: ",l);
num_levels = l;
make_level(current_level);

delay_time = 45;
current_delay = 0;
</string>
